    <script type="text/javascript">
        $(document).ready(function () {
            $('.btnSubmit').on('click', function (e) {
                var form = $("form");
                if (!$(this).hasClass("noloading")) {
                    if (form.length > 0) {
                        if ($("form").valid()) {
                            var $this = $(this);
                            $this.button('loading');
                            setTimeout(function () {
                                $this.button('reset');
                            }, 1500);
                        }
                    }
                    else {
                        var $this = $(this);
                        $this.button('loading');
                        setTimeout(function () {
                            $this.button('reset');
                        }, 1500);
                    }
                }
            });


        });

    </script>


